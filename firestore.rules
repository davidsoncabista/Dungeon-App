rules_version = '2';

service cloud.firestore {
  match /databases/{database}/documents {
    match /{document=**} {
      // ATENÇÃO: Regras abertas apenas para teste.
      // Permite que qualquer usuário autenticado leia e escreva em todo o banco.
      // NÃO USE EM PRODUÇÃO.
      allow read, write: if request.auth != null;
    }
  }
}
